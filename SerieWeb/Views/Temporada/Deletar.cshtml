@model SerieWeb.Models.Temporada

@{
    ViewBag.Title = "Deletar";
}
<div class="container">
    <h2>Deletar Temporada</h2>
    <hr />
    <div>
        <dl class="dl-horizontal">
            <dt>
                Nome serie
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Serie.NomeSerie)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.NomeTemporada)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.NomeTemporada)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Deletar" class="btn btn-danger" /> |
                @Html.ActionLink("Voltar para lista de Séries", "Index", new object { }, new { @class = "badge badge-secondary" })
            </div>
        }
    </div>
</div>
