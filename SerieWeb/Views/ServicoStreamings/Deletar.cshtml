@model SerieWeb.Models.Admininstracao.ServicoStreaming

@{
    ViewBag.Title = "Deletar";
}
<div class="panel-header panel-header-sm">

</div>
<div class="container">
    <div class="content" style="padding: 0; min-height: calc(100vh - 123px); margin-top: -50px;">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <h5 class="title">Deletar Serviço</h5>
                    </div>
                    <div class="card-body">
                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.NomeServicoStreaming)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.NomeServicoStreaming)
                            </dd>

                        </dl>

                        @using (Html.BeginForm())
                        {
                            @Html.AntiForgeryToken()

                            <div class="form-actions no-color float-right" style="margin-right:25px">

                                @Html.ActionLink("Voltar", "Index", new object { }, new { @class = "badge badge-secondary" })
                                <input type="submit" value="Deletar" class="btn btn-danger" />
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

