@model List<SerieWeb.Models.Admininstracao.Serie>
@{
    ViewBag.Title = "Index";
}
<div class="panel-header panel-header-sm">

</div>
<div class="container" style="margin-top:0;">
    <div class="row">
        <div class="col-12">            
            <ul class="nav nav-tabs" id="myTab" role="tablist">
                <li class="nav-item">
                    @Html.ActionLink("Favorito", "Index", "Perfil", new { area = "" }, new { @class = "nav-link active" })
                    
                </li>
                <li class="nav-item">
                    @Html.ActionLink("Calendario", "Calendario", "Perfil", new { area = "" }, new { @class = "nav-link" })

                </li>
                <li class="nav-item">
                    @Html.ActionLink("Servico", "Servico", "Perfil", new { area = "" }, new { @class = "nav-link" })

                </li>
                <li class="nav-item">
                    @Html.ActionLink("Recomendado", "Recomendado", "Perfil", new { area = "" }, new { @class = "nav-link" })

                </li>
            </ul>

        </div>
    </div>
</div>

<div class="container">
    @foreach (var serie in Model)
    {
        <p>@serie.NomeSerie</p>
    }
</div>




<script>
    $('#myTab a').on('click', function (e) {
        e.preventDefault()
        $(this).tab('show')
    })
</script>
